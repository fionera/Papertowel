<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!--suppress XmlServiceArgumentInspection -->
        <service id="website" synthetic="true" public="true" class="Papertowel\Framework\Entity\Website\Website" autowire="false">
        </service>

        <service id="Papertowel\Framework\Modules\Website\Command\CreateWebsiteCommand" autowire="true" public="false">
            <tag name="console.command"/>
        </service>

        <service id="Papertowel\Framework\Modules\Website\Command\ListWebsiteCommand" autowire="true" public="false">
            <tag name="console.command"/>
        </service>

        <service id="Papertowel\Framework\Modules\Website\WebsiteProvider" autowire="true" public="false">
            <argument type="service" id="doctrine"/>
            <argument type="service" id="logger"/>
        </service>
        <service id="papertowel.framework.website.website_provider" alias="Papertowel\Framework\Modules\Website\WebsiteProvider"/>
    </services>
</container>